{
  "name": "jobspot",
  "version": "1.0.0",
  "scripts": {
    "android": "expo start --android",
    "format": "prettier --loglevel warn --write \"**/*.{--write,ts,tsx,css,md}\"",
    "ios": "expo start --ios",
    "start": "expo start",
    "web": "expo start --web",
    "prepare": "husky install",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "pretest": "npm run lint",
    "posttest": "npm run format"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/native": "^6.1.7",
    "@tamagui/font-inter": "1.74.8",
    "@tamagui/react-native-media-driver": "1.74.8",
    "@tamagui/shorthands": "1.74.8",
    "@tamagui/themes": "1.74.8",
    "expo": "~49.0.11",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "tamagui": "1.74.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@release-it/conventional-changelog": "^8.0.1",
    "@tamagui/babel-plugin": "1.74.8",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.3",
    "release-it": "^17.0.1",
    "typescript": "^5.1.3"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {}
          ]
        }
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{--write,ts,tsx,css,md}": "prettier --write"
  },
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0",
    "react-refresh": "~0.14.0"
  },
  "overrides": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0",
    "react-refresh": "~0.14.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true,
  "license": "MIT"
}
