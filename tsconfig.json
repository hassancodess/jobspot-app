{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "checkJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"],
      "@env": ["/src/utils/env.js"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "docs",
    "cli",
    "android",
    "ios",
    "lint-staged.config.js"
  ]
}
